{
  "name": "@sovereign-university/types",
  "version": "0.0.1",
  "type": "module",
  "exports": {
    "development": "./src/index.ts",
    "import": "./dist/src/index.js",
    "types": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc --build tsconfig.lib.json",
    "lint": "eslint --color './**/*.ts'",
    "types:generate": "tsx ./scripts/generate-from-schemas.ts && pnpm prettier --write ./src/generated",
    "type-check": "tsc --build",
    "check-types": "tsc --build",
    "build-types": "tsc --emitDeclarationOnly -p tsconfig.lib.json",
    "trace-performance": "npx tsc --extendedDiagnostics --noEmit -p tsconfig.lib.json > performance.log",
    "clear": "rm -rf ./dist && rm -f *.tsbuildinfo"
  },
  "devDependencies": {
    "@sovereign-university/schemas": "workspace:*",
    "@types/express-session": "^1.18.0",
    "zod": "3.23.8"
  }
}
